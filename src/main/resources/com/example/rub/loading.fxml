<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<HBox alignment="CENTER" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.rub.LoadingController">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
    <Label text="Contatti salvati:" />
    <Label fx:id="savedContacts" text="XXXX" />
    <VBox fx:id="vbox">
        <Label text="Contatti non importati">
           <VBox.margin>
               <Insets left="30.0"/>
           </VBox.margin>
        </Label>
    </VBox>
</HBox>
